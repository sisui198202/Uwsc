procedure ArrPrint(arr[]) //配列の中身をlogファイルで確認_デバッグ
	for i in arr
		print i
	next
fend

procedure ArrClip(hai[]) //すべての配列要素をクリップボードへ_デバッグ
	dim utuwa
	for i in hai
		utuwa=utuwa+i+"<#CR>"
	next
	sendstr(0,utuwa)
fend

procedure ArrMsg(arr[]) //配列の中身をmsgboxで表示_デバッグ
for i = 0 to length(arr)-1
msgbox(i+"が"+arr[i])
next
fend

